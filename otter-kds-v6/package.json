{
  "name": "otter-kds-v6",
  "version": "6.0.0",
  "description": "Enterprise Kitchen Display System with predictive analytics",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "db:migrate": "cd apps/web && npm run db:migrate",
    "db:push": "cd apps/web && npm run db:push",
    "db:seed": "cd apps/web && npm run db:seed"
  },
  "devDependencies": {
    "turbo": "latest",
    "prettier": "^3.1.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "packageManager": "npm@10.2.3",
  "engines": {
    "node": ">=18.0.0"
  }
}